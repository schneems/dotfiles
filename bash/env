#!/usr/bin/env bash

# which -a python

export EDITOR="code -w"
export PATH="/usr/local/bin:/usr/local/sbin:~/bin:~/root/bin:$PATH"
export GOPATH="$HOME/documents/projects/gohomecode"
export PATH="$GOPATH/bin:$PATH"
# export PATH="$(brew --prefix qt@5.5)/bin:$PATH"
export PATH=$PATH:/usr/local/opt/go/libexec/bi

export NVM_DIR="$HOME/.nvm"

export PATH="$HOME/.cargo/bin:$PATH"
export NODE_PATH="/usr/local/lib/node"
export DISABLE_SPRING="true"

# Set PATH, CDPATH, EDITOR, etc.
export CPPFLAGS=-I/opt/X11/include

# rb-env
# eval "$(rbenv init -)"
source /usr/local/opt/chruby/share/chruby/chruby.sh
chruby 2.5 # $(chruby | tail -n 1 | sed s/*//) # grabs the latest installed ruby

